<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.rcpl.tictactoe.gameActivity"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="81dp">

    <Button
        android:id="@+id/TopR"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="102dp"
        android:layout_height="68dp"
        android:layout_marginStart="10dp"
        android:background="#0000cd"
        android:onClick="submit"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBaseline_toBaselineOf="@+id/TopM"
        app:layout_constraintLeft_toRightOf="@+id/TopM"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1" />

    <Button
        android:id="@+id/TopL"
        android:layout_width="102dp"
        android:layout_height="68dp"
        android:layout_marginEnd="10dp"
        android:background="#0000cd"
        android:onClick="submit"
        android:textSize="30sp"
        android:textStyle="bold"
        android:textColor="@android:color/background_light"
        app:layout_constraintBaseline_toBaselineOf="@+id/TopM"
        app:layout_constraintRight_toLeftOf="@+id/TopM"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintRight_creator="1" />

    <Button
        android:id="@+id/TopM"
        android:layout_width="102dp"
        android:layout_height="68dp"
        android:layout_marginTop="29dp"
        android:background="#0000cd"
        android:onClick="submit"
        android:textSize="30sp"
        android:textStyle="bold"
        android:textColor="@android:color/background_light"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <Button
        android:id="@+id/MidL"
        android:layout_width="0dp"
        android:layout_height="68dp"
        android:background="#0000cd"
        android:onClick="submit"
        android:textSize="30sp"
        android:textStyle="bold"
        android:textColor="@android:color/background_light"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/TopL"
        android:layout_marginTop="9dp"
        app:layout_constraintTop_toBottomOf="@+id/TopL"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/TopL" />

    <Button
        android:id="@+id/MidR"
        android:layout_width="0dp"
        android:layout_height="68dp"
        android:layout_marginTop="9dp"
        android:background="#0000cd"
        android:onClick="submit"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="@+id/TopR"
        app:layout_constraintRight_toRightOf="@+id/TopR"
        app:layout_constraintTop_toBottomOf="@+id/TopR"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <Button
        android:id="@+id/MidM"
        android:layout_width="0dp"
        android:layout_height="68dp"
        android:background="#0000cd"
        android:onClick="submit"
        android:textSize="30sp"
        android:textStyle="bold"
        android:textColor="@android:color/background_light"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/TopM"
        android:layout_marginTop="9dp"
        app:layout_constraintTop_toBottomOf="@+id/TopM"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/TopM" />


    <Button
        android:id="@+id/BotL"
        android:layout_width="0dp"
        android:layout_height="68dp"
        android:background="#0000cd"
        android:onClick="submit"
        android:textSize="30sp"
        android:textStyle="bold"
        android:textColor="@android:color/background_light"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/MidL"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/MidL"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/MidL" />

    <Button
        android:id="@+id/BotM"
        android:layout_width="0dp"
        android:layout_height="68dp"
        android:background="#0000cd"
        android:onClick="submit"
        android:textSize="30sp"
        android:textStyle="bold"
        android:textColor="@android:color/background_light"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/MidM"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/MidM"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/MidM" />

    <Button
        android:id="@+id/BotR"
        android:layout_width="0dp"
        android:layout_height="68dp"
        android:layout_marginTop="8dp"
        android:background="#0000cd"
        android:onClick="submit"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="@+id/MidR"
        app:layout_constraintRight_toRightOf="@+id/MidR"
        app:layout_constraintTop_toBottomOf="@+id/MidR"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="52dp"
        android:layout_marginBottom="130dp"
        android:layout_marginEnd="29dp"
        android:layout_marginStart="29dp"
        android:textColor="@android:color/background_dark"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="0dp"
        android:layout_height="65dp"
        android:layout_marginEnd="42dp"
        android:layout_marginStart="42dp"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#ffff00"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintBaseline_toBaselineOf="@+id/MidL"
        app:layout_constraintHorizontal_bias="0.654"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />


</android.support.constraint.ConstraintLayout>
